# 输出模式: cli, matrix, a2a, hybrid
OUTPUT_MODE=cli

# Matrix 配置（matrix/hybrid 模式下需要）
MATRIX_HOMESERVER=http://localhost:6167
MATRIX_ACCESS_TOKEN=replace_me
MATRIX_ROOM_ID=!replace_me:matrix.local

# A2A 配置
AGENT_ID=agent-001
AGENT_NAME=Virtual Agent
A2A_TARGET_AGENT=
A2A_PEER_AGENTS=

# LLM 配置
OPENAI_API_KEY=replace_me
OPENAI_MODEL=gpt-4o-mini

# 存储配置
STORE_TYPE=memory
STORE_PATH=./data
STORE_MAX_SIZE=1000

# 上下文配置
CONTEXT_LIMIT=50
SYSTEM_PROMPT=你是虚拟公司中的无状态智能体。

# CLI 配置
INTERACTIVE=false
CLI_ECHO=true

# 日志配置
LOG_FORMAT=pretty
RUST_LOG=info
